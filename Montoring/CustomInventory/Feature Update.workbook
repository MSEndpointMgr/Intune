          {
            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "WaaSDeploymentStatus\r\n| where TimeGenerated {TimeRange:value}\r\n| where UpdateCategory == \"Feature\"\r\n| where TargetOSVersion in ({TargetOSVersion}) or '*' in ({TargetOSVersion})\r\n| where \"{DeviceName:escape}\" == \"*\" or Computer contains \"{DeviceName:escape}\" or \"{DeviceName:escape}\" == \"All devices\"\r\n| summarize arg_max(TimeGenerated, *) by Computer, TargetOSVersion, DeploymentStatus\r\n| extend filterResult = case(\"{resultName}\" == \"Total\",\"\", \"{resultName}\" == \"Success\", \"Update completed\", \"{resultName}\" == \"Failure\", \"failed\", \"{resultName}\" == \"In progress\", \"In progress\", \"{resultName}\" == \"Unknown\", \"Unknown\", \"{resultName}\" == \"Progress stalled\", \"Progress stalled\",\"Others\")\r\n| where DeploymentStatus contains filterResult\r\n| extend DeployStatus = case(DeploymentStatus == \"Update completed\", \"‚úîÔ∏è Update completed\",                                     \r\n    DeploymentStatus == \"Failed\", \"‚ùå Failed\",                                     \r\n    DeploymentStatus == \"Unknown\", \"‚ö†Ô∏è Unknown\",\r\n    DeploymentStatus == \"In progress\", \"üîò In progress\",\r\n    DeploymentStatus == \"Progress stalled\", \"üïí Progress stalled\",                                              \r\n    DeploymentStatus == \"\", \"‚ö†Ô∏è Unknown\", \r\n    \"üö´ Others\") \r\n| project TimeGenerated, LastScan, Computer, ['Deployment Status'] = DeployStatus, DetailedStatus, DeploymentError, CurrentOSVersion = OSVersion, TargetOSVersion, DeferralDays, PauseState, ExpectedInstallDate\r\n| sort by LastScan, Computer desc\r\n\r\n\r\n",
